{
    "name": "krismawan4/inertiajs-query-builder-tables",
    "description": "Inertia.js Front-end Components for Spatie's Laravel Query Builder, with support for Laravel 12",
    "homepage": "https://github.com/krismawan4/inertiajs-query-builder-tables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krismawan",
            "email": "krismawan4@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.37|^10.0|^11.0|^12.0",
        "spatie/laravel-query-builder": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "inertiajs/inertia-laravel": "^0.6.9|^1.0|^2.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Krismawan4\\InertiaTables\\": "php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krismawan4\\InertiaTables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --verbose"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Krismawan4\\InertiaTables\\InertiaTableServiceProvider"
            ]
        }
    }
}
